{
  "manifest_version": 3,
  "name": "Ipynb Files Viewer",
  "version": "1.1.0",
  "description": "Open Jupyter notebooks (.ipynb) right in your browser — no downloads or extra tools needed.",
  "author": "Anton Borzenko",

  "permissions": [
      "webRequest",
      "background",
      "downloads",
      "tabs"
  ],
  "host_permissions": [
      "<all_urls>"
  ],
  "background": {
    "service_worker": "ipynbHandler.js",
    "type": "module"
  },
  "action": {
    "default_icon": "icon.png"
  }
}
